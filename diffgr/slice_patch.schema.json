{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "DiffGR Slice Patch",
  "type": "object",
  "additionalProperties": false,
  "required": ["rename", "move"],
  "properties": {
    "rename": {
      "type": "object",
      "additionalProperties": {
        "type": "string",
        "minLength": 1
      },
      "default": {}
    },
    "move": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["chunk", "to"],
        "properties": {
          "chunk": { "type": "string", "minLength": 1 },
          "to": { "type": "string", "minLength": 1 }
        }
      },
      "default": []
    }
  }
}
